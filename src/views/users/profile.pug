extends ../base
include ../mixins/video

block content 
    div.profile_wrap
        if loggedInUser.socialOnly
            img(src=user.avatarUrl).avatar
        else
            img(src="/" + user.avatarUrl).avatar
        h3=user.name
        div.video_grid
            each video  in user.videos
                +video(video)
            else
                div.empty__message Sorry nothing fonud.